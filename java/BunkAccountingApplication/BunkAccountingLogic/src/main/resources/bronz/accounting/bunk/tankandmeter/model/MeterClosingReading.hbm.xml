<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bronz.accounting.bunk.tankandmeter.model.MeterClosingReading">
        <id name="slNo" type="java.lang.Integer">
            <column name="PK_SLNO" />
            <generator class="increment"></generator>
        </id>
        <property name="meterId" type="java.lang.Integer">
            <column name="METER_ID"/>
        </property>
        <property name="tankId" type="java.lang.Integer">
            <column name="TANK_ID"/>
        </property>
        <property name="productId" type="java.lang.Integer">
            <column name="PROD_ID"/>
        </property>
        <property name="meterName" type="java.lang.String">
            <column name="METER_NAME" />
        </property>
        <property name="finalReading" type="java.math.BigDecimal">
            <column name="FINAL_READING" />
        </property>
        <property name="date" type="java.lang.Integer">
            <column name="DATE" />
        </property>
    </class>
</hibernate-mapping>
