<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bronz.accounting.bunk.tankandmeter.model.MeterTransaction" table="pbms_closing_readings">
        <id name="slNo" type="java.lang.Integer">
            <column name="PK_SLNO" />
            <generator class="increment"></generator>
        </id>
        <property name="meterId" type="java.lang.Integer">
            <column name="PK_METER_ID"/>
        </property>
        <property name="date" type="java.lang.Integer">
            <column name="DATE"/>
        </property>
        <property name="detail" type="java.lang.String">
            <column name="DETAIL" />
        </property>
        <property name="finalReading" type="java.math.BigDecimal">
            <column name="FINAL_READING" />
        </property>
        <property name="meterSale" type="java.math.BigDecimal">
            <column name="METER_SALE" />
        </property>
    </class>
</hibernate-mapping>
