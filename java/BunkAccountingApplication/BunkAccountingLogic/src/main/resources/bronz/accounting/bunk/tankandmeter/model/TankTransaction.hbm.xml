<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bronz.accounting.bunk.tankandmeter.model.TankTransaction" table="pbms_tank_transactions">
        <id name="slNo" type="java.lang.Integer">
            <column name="PK_SLNO" />
            <generator class="increment"></generator>
        </id>
        <property name="tankId" type="java.lang.Integer">
            <column name="TANK_ID"/>
        </property>
        <property name="date" type="java.lang.Integer">
            <column name="DATE"/>
        </property>
        <property name="transType" type="java.lang.String">
            <column name="TRANS_TYPE"/>
        </property>
        <property name="detail" type="java.lang.String">
            <column name="DETAIL" />
        </property>
        <property name="quantity" type="java.math.BigDecimal">
            <column name="QUANTITY" />
        </property>
        <property name="balance" type="java.math.BigDecimal">
            <column name="BALANCE" />
        </property>
        <property name="density" type="java.math.BigDecimal">
            <column name="DENSITY" />
        </property>
    </class>
</hibernate-mapping>
