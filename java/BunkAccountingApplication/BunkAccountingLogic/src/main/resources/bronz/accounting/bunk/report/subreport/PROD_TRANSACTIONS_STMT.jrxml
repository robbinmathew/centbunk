	</variable>
	<variable name="totalCredit" class="java.math.BigDecimal" calculation="Sum" resetType="Report">
		<variableExpression><![CDATA[bronz.accounting.bunk.products.model.ProductTransaction.CREDIT_TRANS_TYPES.contains($F{TYPE})?java.math.BigDecimal.ZERO:$F{QUANTITY}]]></variableExpression>
